{
  "name": "@schibstedspain/sui-svgiconset",
  "version": "1.0.2",
  "description": "ReactJS SUI SVG iconset",
  "main": "lib/",
  "scripts": {
    "prepublish": "npm run lib && npm run dist",
    "clean:dist": "rimraf ./dist/*",
    "clean:lib": "rimraf ./lib/*",
    "predist": "npm run clean:dist -s",
    "dist": "webpack",
    "predoc": "rimraf ./docs/dist/*",
    "doc": "npm run dist && npm run doc:copy && npm run doc:publish",
    "doc:copy": "copyfiles ./dist/* ./docs/",
    "doc:publish": "gh-pages -d docs",
    "prelib": "npm run clean:lib -s",
    "lib": "npm run lib:scripts && npm run lib:styles",
    "lib:scripts": "babel src --out-dir lib",
    "start": "npm run start:open && npm run start:server",
    "start:server": "webpack-dev-server",
    "start:open": "opener http://localhost:8080",
    "phoenix": "rimraf ./node_modules/ && npm install",
    "lint:js": "node_modules/.bin/eslint --ext .js,.jsx src/",
    "lint:sass": "node_modules/.bin/sass-lint src/**/*.scss -c -v"
  },
  "author": "David Garcia",
  "license": "ISC",
  "devDependencies": {
    "@schibstedspain/frontend-pre-commit-rules": "7.0",
    "babel-cli": "6.14.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "^6.16.0",
    "camelcase": "^3.0.0",
    "copyfiles": "1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "opener": "1.4.2",
    "react-hot-loader": "3.0.0-beta.4",
    "rimraf": "2.5.4",
    "svg-inline-loader": "0.6.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.15.2",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "react": "15.3.1"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
